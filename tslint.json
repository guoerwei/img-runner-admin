{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-react",
    "tslint-react-hooks",
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [
      false,
      "single",
      "jsx-double"
    ],
    "no-console": [
      false
    ],
    "no-bitwise": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "react-hooks-nesting": "error",
    "prettier": true
  }
}